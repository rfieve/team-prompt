{
    "name": "@romainfieve/team-prompt",
    "version": "0.0.35",
    "description": "A TypeScript tool to create prompt based on team members.",
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "docs": "typedoc src/index.ts",
        "build": "tsup",
        "lint": "eslint --fix ./src/ --ext .js --ext .ts --ext .tsx --format table",
        "test": "NODE_PATH=./ jest --coverage --passWithNoTests",
        "test:watch": "NODE_PATH=./ jest --watch --coverage --passWithNoTests",
        "prepublish": "tsup"
    },
    "author": "rfieve",
    "license": "MIT",
    "keywords": [
        "typescript",
        "team-prompt",
        "utility"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/rfieve/team-prompt.git"
    },
    "homepage": "https://github.com/rfieve/team-prompt#readme",
    "bugs": {
        "url": "https://github.com/rfieve/team-prompt/issues"
    },
    "devDependencies": {
        "@jest/types": "^29.6.3",
        "@types/jest": "^29.5.3",
        "@typescript-eslint/eslint-plugin": "^6.4.1",
        "@typescript-eslint/parser": "^6.4.1",
        "eslint": "^8.47.0",
        "eslint-formatter-table": "^7.32.1",
        "eslint-plugin-import": "^2.28.1",
        "eslint-plugin-typescript-sort-keys": "^2.3.0",
        "eslint-plugin-unicorn": "^48.0.1",
        "jest": "^29.6.3",
        "prettier": "^3.0.2",
        "ts-jest": "^29.1.1",
        "ts-node": "^10.9.1",
        "tsup": "^7.2.0",
        "typedoc": "^0.25.1",
        "typescript": "^5.1.6"
    }
